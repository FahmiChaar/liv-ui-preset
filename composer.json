{
  "name": "liv/liv",
  "description": "A Laravel frontend preset to get you up and running with Inertia.js and Vuetify",
  "keywords": ["laravel", "preset", "inertiajs"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohamed Fahmi Chaar",
      "email": "fahmichaar@gmail.com"
    }
  ],
  "require": {
    "infyomlabs/laravel-generator": "8.0.x-dev",
    "doctrine/dbal": "^3.3"
  },
  "require-dev": {
    "orchestra/testbench": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "Liv\\InertiaJsPreset\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Liv\\InertiaJsPreset\\InertiaJsPresetServiceProvider"
      ]
    }
  }
}
